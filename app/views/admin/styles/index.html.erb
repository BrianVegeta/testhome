<h1>Listing admin_styles</h1>
<div id="style_editor_frame" style="height: 600px; width: 1100px; border: 1px solid black; ">
  <iframe src="<%= new_admin_style_path(organization_id: params[:organization_id]) %>" width="100%" height="100%"></iframe>
</div>





<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_styles.each do |admin_style| %>
      <tr>
        <td><%= link_to 'Show', admin_style %></td>
        <td><%= link_to 'Edit', edit_admin_style_path(admin_style) %></td>
        <td><%= link_to 'Destroy', admin_style, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Style', new_admin_style_path %>
