<h1>Listing address_item_streets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @address_item_streets.each do |address_item_street| %>
      <tr>
        <td><%= link_to 'Show', address_item_street %></td>
        <td><%= link_to 'Edit', edit_address_item_street_path(address_item_street) %></td>
        <td><%= link_to 'Destroy', address_item_street, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Item street', new_address_item_street_path %>
